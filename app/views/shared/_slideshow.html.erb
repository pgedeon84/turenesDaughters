<div class="parallax2" style="background-image: url('<%= image_path "ladyinorange.jpg" %>')"></div>
<br>
<h4 class="color-magenta" style="text-align:center">Street Fashion Gallery</h4>
<div class="container">
  <div class="mySlides">
    <div class="numbertext">1 / 6</div>
    <%= cl_image_tag "lgvczpkz117ohz7wohyl.jpg", alt: "Fashion Styles", class: "imgExpanded", style: "width: 100%"%>
  </div>

  <div class="mySlides">
    <div class="numbertext">2 / 6</div>
    <%= cl_image_tag "u5bvpsptmyru50dobgxl.jpg", alt: "Fashion Styles", class: "imgExpanded", style: "width: 100%"%>
  </div>

  <div class="mySlides">
    <div class="numbertext">3 / 6</div>
    <%= cl_image_tag "m9agrmwg1m8zkc5kewgu.jpg", alt: "Fashion Styles", class: "imgExpanded", style: "width: 100%"%>
  </div>

  <div class="mySlides">
    <div class="numbertext">4 / 6</div>
    <%= cl_image_tag "r703luxo8g93u7fzu31p.jpg", alt: "Fashion Styles", class: "imgExpanded", style: "width: 100%"%>
  </div>

  <div class="mySlides">
    <div class="numbertext">5 / 6</div>
    <%= cl_image_tag "rubecl1kdvvuyhneayy8.jpg", alt: "Fashion Styles", class: "imgExpanded", style: "width: 100%"%>
  </div>

  <div class="mySlides">
    <div class="numbertext">6 / 6</div>
    <%= cl_image_tag "nqmtnmsa9sfzscbd8kin.jpg", alt: "Fashion Styles",  class: "imgExpanded", style: "width: 100%"%>
  </div>

  <a class="prev" onclick="plusSlides(-1)"><span class="color-magenta">❮</span></a>
  <a class="next" onclick="plusSlides(1)"><span class="color-magenta">❯</span></a>

  <div class="caption-container">
    <p id="caption"></p>
  </div>
  <div class="container">
    <div class="slideshow-row">
      <div class="slideshow-column">
        <%= cl_image_tag "lgvczpkz117ohz7wohyl.jpg", alt: "Fashion Styles", class: "demo cursor", style: "width: 100%", onclick: "currentSlide(1)"%>
      </div>
      <div class="slideshow-column">
        <%= cl_image_tag "u5bvpsptmyru50dobgxl.jpg", alt: "Fashion Styles", class: "demo cursor", style: "width: 100%", onclick: "currentSlide(2)"%>
      </div>
      <div class="slideshow-column">
        <%= cl_image_tag "m9agrmwg1m8zkc5kewgu.jpg", alt: "Fashion Styles", class: "demo cursor", style: "width: 100%", onclick: "currentSlide(3)"%>
      </div>
      <div class="slideshow-column">
        <%= cl_image_tag "r703luxo8g93u7fzu31p.jpg", alt: "Fashion Styles", class: "demo cursor", style: "width: 100%", onclick: "currentSlide(4)"%>
      </div>
      <div class="slideshow-column">
        <%= cl_image_tag "rubecl1kdvvuyhneayy8.jpg", alt: "Fashion Styles", class: "demo cursor", style: "width: 100%", onclick: "currentSlide(5)"%>
      </div>
      <div class="slideshow-column">
        <%= cl_image_tag "nqmtnmsa9sfzscbd8kin.jpg", alt: "Fashion Styles",  class: "demo cursor", style: "width: 100%", onclick: "currentSlide(6)"%>
      </div>
    </div>
  </div>
  <br>
</div>

<script>
var slideIndex = 1;
showSlides(slideIndex);

function plusSlides(n) {
  showSlides(slideIndex += n);
}

function currentSlide(n) {
  showSlides(slideIndex = n);
}

function showSlides(n) {
  var i;
  var slides = document.getElementsByClassName("mySlides");
  var dots = document.getElementsByClassName("demo");
  var captionText = document.getElementById("caption");
  if (n > slides.length) {slideIndex = 1}
  if (n < 1) {slideIndex = slides.length}
  for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none";
  }
  for (i = 0; i < dots.length; i++) {
      dots[i].className = dots[i].className.replace(" active", "");
  }
  slides[slideIndex-1].style.display = "block";
  dots[slideIndex-1].className += " active";
  captionText.innerHTML = dots[slideIndex-1].alt;
}
</script>
